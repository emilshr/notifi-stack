{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -e .env turbo dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run lint build --filter='@emilshr/*' && changeset publish",
    "setup-db": "yarn workspace web run setup-db",
    "run-type-check": "yarn workspace web run type-check"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.7",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.45.0",
    "eslint-config-custom": "*",
    "prettier": "^3.0.0",
    "turbo": "^1.10.12"
  },
  "name": "notifi-stack",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@changesets/cli": "^2.26.2"
  },
  "packageManager": "yarn@3.3.1"
}
