{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -e .env turbo dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run lint build --filter='@emilshr/*' && changeset publish",
    "setup-db": "env-cmd -f .env npm --workspace=web run setup-db",
    "type-check": "env-cmd -f .env  npm --workspace=web run type-check",
    "lint-check": "env-cmd -f .env  npm run lint",
    "open-studio": "env-cmd -f .env  npm --workspace=web run studio"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@turbo/gen": "^1.10.7",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.45.0",
    "eslint-config-custom": "*",
    "prettier": "^3.0.2",
    "prisma": "^5.3.1",
    "turbo": "^1.10.13"
  },
  "name": "notifi-stack",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "razorpay": "^2.9.2"
  }
}
