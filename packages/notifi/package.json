{
  "name": "@emilshr/notifi",
  "version": "0.0.1",
  "description": "A wrapper for your react application that can monitor and give you an analysis on the app traffic",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "issues": "https://github.com/emilshr/notifi-stack/issues",
  "homepage": "https://github.com/emilshr/notifi-stack",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup --config ./tsup.config.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint 'src/**/*.ts*'",
    "dev": "npm run build -- --watch"
  },
  "keywords": [
    "notifi",
    "package",
    "react"
  ],
  "author": "emilshr",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.45.0",
    "eslint-config-custom": "*",
    "react": "^18.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
